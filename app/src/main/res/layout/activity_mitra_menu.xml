<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MitraMenu">

    <LinearLayout
        android:id="@+id/listmitra"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/Asuransi"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_below="@+id/textView3"
                android:layout_alignParentEnd="true"
                android:gravity="center"
                android:text="Asuransi"
                android:textColor="@color/contact_number"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/Institusi"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_below="@+id/textView3"
                android:layout_alignParentEnd="true"
                android:gravity="center"
                android:text="Institusi"
                android:textColor="@color/contact_number"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/Corporate"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_below="@+id/textView3"
                android:layout_alignParentEnd="true"
                android:gravity="center"
                android:text="Corporate"
                android:textColor="@color/contact_number"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/MitraClass"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"></LinearLayout>
    </LinearLayout>


</LinearLayout>